<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/medical_primary_light">
    
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/medical_primary_light" />
            <corners android:radius="20dp" />
            <stroke 
                android:width="3dp" 
                android:color="@color/medical_primary" />
            <padding 
                android:left="16dp" 
                android:right="16dp" 
                android:top="20dp" 
                android:bottom="20dp" />
        </shape>
    </item>
    
</ripple>
